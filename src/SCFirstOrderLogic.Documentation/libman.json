{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.9.1",
      "destination": "wwwroot/css/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "google-code-prettify@1.0.5",
      "destination": "wwwroot/lib/google-code-prettify/",
      "files": [
        "bin/prettify.min.css",
        "bin/prettify.min.js",
        "bin/run_prettify.min.js"
      ]
    }
  ]
}